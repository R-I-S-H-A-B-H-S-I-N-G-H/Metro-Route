{
	"expo": {
		"name": "metro-route",
		"slug": "metro-route",
		"version": "1.0.0",
		"orientation": "portrait",
		"splash": {
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#FFFFFF"
			},
			"package": "com.helix.metroroute"
		},
		"web": {
			"favicon": "./assets/favicon.png"
		}
	}
}
